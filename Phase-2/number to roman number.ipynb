    "n=int(input(\"Enter Number : \"))\n",
    "r=['M','CM','D','CD','C','XC','L','XL','X','IX','V','IV','I']\n",
    "v=[1000,900,500,400,100,90,50,40,10,9,5,4,1]\n",
    "ans=''\n",
    "i=0\n",
    "while n>0:\n",
    "    for j in range(n//v[i]):\n",
    "        ans=ans+r[i]\n",
    "        n=n-v[i]\n",
    "    i=i+1\n",
    "print(ans)\n"
